<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="11" id="br.com.pedbarros.prevtev" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PrevTev</name>
    <description>App para tratamento educacional do TEV</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <icon density="mdpi" src="../src/statics/icons/logo-mdpi.png" />
    <icon density="hdpi" src="../src/statics/icons/logo-xhdpi.png" />
    <icon density="xhdpi" src="../src/statics/icons/logo-xhdpi.png" />
    <icon density="xxhdpi" src="../src/statics/icons/logo-xxhdpi.png" />
    <icon density="xxxhdpi" src="../src/statics/icons/logo-xxxhdpi.png" />
    <preference name="SplashScreenDelay" value="2000" />
    <preference name="SplashScreenSpinnerColor" value="white" />
    <splash density="land-hdpi" src="../src/statics/splash/splash-xxxhdpi.png" />
    <splash density="land-ldpi" src="../src/statics/splash/splash-xxxhdpi.png" />
    <splash density="land-mdpi" src="../src/statics/splash/splash-xxxhdpi.png" />
    <splash density="land-xhdpi" src="../src/statics/splash/splash-xxxhdpi.png" />
    <splash density="land-xxhdpi" src="../src/statics/splash/splash-xxhdpi.png" />
    <splash density="land-xxxhdpi" src="../src/statics/splash/splash-xxxhdpi.png" />
    <splash density="hdpi" src="../src/statics/splash/splash-xxxhdpi.png" />
    <splash density="ldpi" src="../src/statics/splash/splash-xxxhdpi.png" />
    <splash density="mdpi" src="../src/statics/splash/splash-xxxhdpi.png" />
    <splash density="xhdpi" src="../src/statics/splash/splash-xxxhdpi.png" />
    <splash density="xxhdpi" src="../src/statics/splash/splash-xxxhdpi.png" />
    <splash density="xxxhdpi" src="../src/statics/splash/splash-xxxhdpi.png" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
